def insertion_sort(A):
    for j in range(1, len(A)):
        key = A[j]
        # Insert A[j] into the sorted sequence A[0...j-1]
        i = j - 1
        while i >= 0 and A[i] > key:
            A[i + 1] = A[i]
            i -= 1
        A[i + 1] = key
        
        
arr = [5, 2, 4, 6, 1, 3]
print("Antes de ordenar:", arr)
insertion_sort(arr)
print("DespuÃ©s de ordenar:", arr)

